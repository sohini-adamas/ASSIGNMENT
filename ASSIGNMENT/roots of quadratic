#include <stdio.h>
#include<math.h>
int main() {
    int a,b,c,d;
    printf("assuming quadratic equation is in the form ax^2+bx+c enter values of a,b and c:");
    scanf("%d %d %d",&a,&b,&c);
    d=(b*b-4*a*c);
    float e= (-b+sqrt(d))/2*a;
    float f= (-b-sqrt(d))/2*a;
    printf("roots are %f and %f",e,f);
}
